{
  "name": "relay-graphql-react",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/graphql": {
      "target": "http://localhost:4000"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "history": "^4.7.2",
    "mongoose": "^5.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-relay": "^1.6.0",
    "react-scripts": "1.1.4",
    "relay-runtime": "^1.6.0",
    "universal-router": "^6.0.0"
  },
  "scripts": {
    "client": "node ./setup && react-scripts start",
    "build": "node ./setup && react-scripts build",
    "test": "node ./setup && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src/ --schema ./server/schema/schema.graphql",
    "server": "nodemon ./server/server.js",
    "update-schema": "node ./server/updateSchema.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-relay": "^1.6.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "nodemon": "^1.17.5",
    "relay-compiler": "^1.6.0"
  }
}
